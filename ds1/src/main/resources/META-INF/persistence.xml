<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="driveshare">
		<class>com.shpitc.driveshare.shared.model.jpa.Account</class>
		<class>com.shpitc.driveshare.shared.model.jpa.Company</class>
		<class>com.shpitc.driveshare.shared.model.jpa.Garage</class>
		<class>com.shpitc.driveshare.shared.model.jpa.GarageUser</class>
		<class>com.shpitc.driveshare.shared.model.jpa.Make</class>
		<class>com.shpitc.driveshare.shared.model.jpa.Model</class>
		<class>com.shpitc.driveshare.shared.model.jpa.ObdData</class>
		<class>com.shpitc.driveshare.shared.model.jpa.Owner</class>
		<class>com.shpitc.driveshare.shared.model.jpa.Person</class>
		<class>com.shpitc.driveshare.shared.model.jpa.Vehicle</class>
		<class>com.shpitc.driveshare.shared.model.jpa.VehicleGarage</class>
		<class>com.shpitc.driveshare.shared.model.jpa.VehicleGaragePK</class>
		<class>com.shpitc.driveshare.shared.model.jpa.VehicleOwner</class>
		<class>com.shpitc.driveshare.shared.model.jpa.VehicleOwnerPK</class>
		<properties>
			<property name="hibernate.cache.use_second_level_cache" value="true"/>
			<property name="hibernate.cache.provider_class" value="net.sf.ehcache.hibernate.EhCacheProvider"/>
		</properties>
	</persistence-unit>
</persistence>